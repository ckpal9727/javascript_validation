<?php
$ans="";
    if(isset($_POST['submit']))
    {
	if((isset($_POST['num1']))&&(isset($_POST['num2'])))
	{
			$num1=$_POST['num1'];
			$num2=$_POST['num2'];
			settype($num1,"integer");
			settype($num2,"integer");
			$op=$_POST['op'];
		switch($op)
		{
			case "+":
				$ans=$num1+$num2;
				break;
				
			case "-":
				$ans=$num1+$num2;
				break;
				
			case "*":
				$ans=$num1+$num2;
				break;
				
			case "/":
				$ans=$num1+$num2;
				break;
				
			case "%":
				$ans=$num1+$num2;
				break;
		}
	}
			

    }
	else
	{
		$num1="";
		$num2="";
		
	}
?>
  <body>
      <form method="post" action="">
               <div class="container d-flex justify-content-center bg-light">
                   <table>
                       <tr>
                           <td>
                               Number1
                           </td>
                           <td>
                               <input type="text" class="form-control" value="<?php echo $num1?>" name="num1">
                           </td>
                       </tr>
                        <tr>
                           <td>
                               Number2
                           </td>
                           <td>
                               <input type="text" class="form-control" value="<?php echo $num2?>" name="num2">
                           </td>
                       </tr>
                       <tr>
                           <td>Operations</td>
                           <td>
                               <select  id="" name="op" class="form-control text-center">
                                <option value="+"><b>+</b></option>
                                <option value="-"><b>-</b></option>
                                <option value="*"><b>*</b></option></option>
                                <option value="/"><b>/</b></option>
                                <option value="%"><b>%</b></option>
                               </select>
                           </td>
                       </tr>
                       <tr><td>answere</td>
                           <td>
                              <input type="text" class="form-control" value="<?php   echo $ans; ?>">
                           </td>
                           
                       </tr>
                       <tr>
                           <td><input type="submit" name="submit" class=" btn btn-primary form-control"></td></td>
                       </tr>
                   </table>

               </div>        

</form> 
  

