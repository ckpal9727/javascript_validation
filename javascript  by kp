<html>
<body>
<head>
<?php


    if(isset($_POST['s']))
    {
		if((isset($_POST['num1']))&& (isset($_POST['num2'])) && (isset($_POST['num3'])))
		{
        $num1=$_POST['num1'];
        $num2=$_POST['num2'];
		$num3=$_POST['num3'];
		
		if($num1==$num2 && $num1==$num3)
		{
			echo("all are same");
		}
		
		if($num1<$num2 && $num1<$num3)
		{
			if($num2<$num3)
			{
		
				echo $num1,$num2,$num3;
				
			}
			else
			{
				echo $num1,$num3,$num2;
			}
		}
		if($num2<$num1 && $num2<$num3)
		{
			if($num1<$num3)
			{
	
				echo $num2,$num1,$num3;
				
			}
			else
			{
				echo $num2,$num3,$num1;
			}
		}
		if($num3<$num1 && $num3<$num2)
		{
			if($num1<$num2)
			{
				echo $num3,$num1,$num2;
				
			}
			else
			{
				echo $num3,$num2,$num1;
			}
		}
			
		
     }
        
	
	}
		else{
			$num1="";
			$num2="";
			$num3="";
		}
?>
</head>


<h2>assendind mannare</h2>

