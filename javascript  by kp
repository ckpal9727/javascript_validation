<?php
$num1;
$num2;
$ans;

    if(isset($_POST['s']))
    {
        $num1=$_POST['num1'];
        $num2=$_POST['num2'];
     
        $op=$_POST['op'];
        if($op=='+')
        {
            $ans=$num1+$num2;
        }
         elseif($op=='-')
        {
            $ans=$num1-$num2;
        }
         elseif($op=='*')
        {
            $ans=$num1*$num2;
        }
         elseif($op=='/')
        {
            $ans=$num1/$num2;
        }
         elseif($op=='%')
        {
            $ans=$num1%$num2;
        }
      
        

    }
	else{
			$num1="";
			$num2="";
			$ans="";
	}
?>


<h2>HTML Forms</h2>

<form action="" method="post">
  
  number2:<input type="text" id="no1" name="num1" value="<?php echo $num1 ?>">><br>
  <br>
  number2:<input type="text" id="no2" name="num2" value="<?php echo $num2 ?>">><br>
  <br>
    <select  id="" name="op" >
                                <option value="+"><b>+</b></option>
                                <option value="-">-</option>
                                <option value="*">*</option></option>
                                <option value="/">/</option>
                                <option value="%">%</option>
                               </select>
  <input type="submit" value="Submit" name="s">
</form> 
<br>
 result:<input type="text" id="ans" name="ans" value="<?php echo $ans ?>"><br>

</body>
</html>




