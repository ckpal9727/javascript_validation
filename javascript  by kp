<?php
 if(isset($_POST['submit']))
 { $num1=$_POST['num1'];
     $num2=$_POST['num2']; 
     $num3=$_POST['num3']; 

     echo $num1."<br>";
     echo $num2."<br>";
     echo $num3."<br>"; 

     echo gettype($num1);
     settype($num1,"integer");
     settype($num2,"integer");
     settype($num3,"integer");
     echo "<br>";
     echo gettype($num1);

     if($num1>$num2 && $num1>$num3)
     {
         if($num2>$num3)
         {
             echo"<br>".$num1." ".$num2." ".$num3;
         }
         else
         {
              echo"<br>".$num1." ".$num3." ".$num2;
         }
     }
       if($num1<$num2 && $num2>$num3)
     {
         if($num1>$num3)
         {
             echo"<br>".$num2." ".$num1." ".$num3;
         }
         else
         {
              echo"<br>".$num2." ".$num3." ".$num1;
         }
     }  if($num3>$num2 && $num1<$num3)
     {
         if($num2>$num1)
         {
             echo"<br>".$num3." ".$num2." ".$num1;
         }
         else
         {
              echo"<br>".$num3." ".$num1." ".$num2;
         }
     }
    } 
    ?>

</html>



