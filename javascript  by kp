<html>
<body>
<head>
</head>
<?php
		$foc="";
		$c="";
		$dm="";
		$rdbms="";
		$php="";

	if(isset($_POST['s']))
	{
		$foc=$_POST['foc'];
		$c=$_POST['c'];
		$dm=$_POST['dm'];
		$rdbms=$_POST['rdbms'];
		$php=$_POST['php'];
		
		echo "total:".($foc+$c+$dm+$rdbms+$php) ."<br>";
		$total=$foc+$c+$dm+$rdbms+$php;
		echo "percentage : ".(($foc+$c+$dm+$rdbms+$php)*100)/500 ."<br>";
		$gred=(($foc+$c+$dm+$rdbms+$php)*100)/500;
		
		if($gred>60)
        {
            echo("first div");
        }
         elseif($gred>=45 && $gred<=59)
        {
            echo("secound div");
        }
         elseif($gred>=33 && $gred<=44)
        {
            echo("third div");
        }
         elseif($gred<33)
        {
             echo("fail");
        }
    		
	}
	else
	{
		$foc="";
		$c="";
		$dm="";
		$rdbms="";
		$php="";
		
	}

?>



<h2></h2>

<form action="" method="post">
  
  C:<input type="text" id="no1" name="c" value="<?php echo $c; ?>"><br>
  <br>
  FOC:<input type="text" id="no2" name="foc" value="<?php echo $foc; ?>"><br>
  <br> 
  DM:<input type="text" id="no2" name="dm" value="<?php echo $dm; ?>"><br>
  <br>
  RDBMS:<input type="text" id="no2" name="rdbms" value="<?php echo $rdbms; ?>"><br>
  <br> 
  PHP:<input type="text" id="no2" name="php" value="<?php echo $php; ?>"><br>
  <br>
 
    
  <input type="submit" value="Submit" name="s">
</form> 


</body>
</html>
