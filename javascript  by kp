<html>
	<head>
		<title>full Name</title>
		<script type="text/javascript">
			var fn,ln,mn;
			
			function data()
			{
			fn=document.getElementById('fn').value;
			ln=document.getElementById('ln').value;
			mn=document.getElementById('mn').value;
				/*alert(fn);
				alert(ln);
				alert(mn);*/
			}
			function getdata()
			{
				alert(" I am in");
					var msg;
					msg="Hii good morning";
					msg+=' '+document.getElementById('fn').value+' '+document.getElementById('ln').value+' '+document.getElementById('mn').value;
					document.getElementById('demo').innerHTML=msg;
					return false;
			}
		</script>
	</head>
	<body>
	
		<H1>Displaying full name  </h1>
		<form action="" method="" onsubmit="return getdata()">
		FName:<input type="text" id="fn"><br>
		LName:<input type="text" id="ln"><br>
		MName:<input type="text" id="mn"><br>
		<input type="submit" value="submit">
		</form>
	<div id="demo"></div>
	</body>
</html>



----------------------day-2-------------------------------------------------
swap program
<html>
	<head>
		<title>settype</title>
	</head>
	<body>
		<form action="" method="get">
			Number1:<input type="text" name="num1">
			Number2:<input type="text" name="num2">
			
		<input type="submit" value="submit" name="submit">
		</form>
	</body>
	<?php
	
		if(isset($_GET['submit']))
		{
			$number1=$_GET['num1'];
			$number2=$_GET['num2'];
			echo $number1;
			echo "<br>";
			echo $number2;
			echo "<br>";
			$swap=$number1;
			$number1=$number2;
			$number2=$swap;
			echo $number1;
			echo "<br>";
			echo $number2;
		}
				
	?>

</html>
--------------------------------------settype and gettype -----------------------

<html>
	<head>
		<title>settype</title>
	</head>
	<body>
		<form action="" method="post">
			name:<input type="text" name="fname">
			
			lname:<input type="text" name="lname">
			age:<input type="text" name="age">
		<input type="submit" value="submit" name="submit">
		</form>
	</body>
	<?php
	
		if(isset($_POST['submit']))
		{
			$fname=$_POST['fname'];
			$lname=$_POST['lname'];
			$age=$_POST['age'];
			
			settype($age,"integer");
			
			
			echo var_dump($age);
			echo var_dump($fname);
			echo gettype($fname);
			echo gettype($lname);
			echo gettype($age);
		}
	?>

</html>
