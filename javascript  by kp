<?php
if(isset($_POST['submit']))
{
    $c=$_POST['c'];
    $foc=$_POST['foc'];
    $dm=$_POST['dm'];
    $rdbms=$_POST['rdbms'];
    $php=$_POST['php'];
    settype($c,"integer");
    settype($foc,"integer");
    settype($dm,"integer");
    settype($rdbms,"integer");
    settype($php,"integer");

    $total=$c+$foc+$dm+$rdbms+$php;
    
    $per=($total*100)/500;
    echo "<br>Your total is ".$total;
    echo "<br>Percentage is ".$per;
    echo "<br> type of total is ".gettype($per);

    if($per>60)
    {
        echo "<br> Your grade is <b>First division</b>";
    }
     elseif($per>45 && $per<59)
    {
        echo "<br> Your grade is <b>second division</b>";
    }
     elseif($per>33 && $per<44)
    {
        echo "<br> Your grade is <b>third division</b>";
    }
    else
    {
        echo "<br> Your grade is<b>fail</b>";
    }
echo "<br>total<input type='text' value=' $total'/>";

    
}

?>
</html>
